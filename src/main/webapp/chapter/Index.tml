<html t:type="booklayout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter" t:title="prop:book?.title" t:bookId="book?.id">


<div class="entry-content commentable">
	<header>
	<h2 class="entry-title"><a href="#" rel="bookmark" title="Permalink to this PAGE">${chapter?.title}</a></h2>
		<nav id="book-nav">
		<ol>
			<li id="nav-left">&lt; Table of Contents</li>
			<li id="nav-right">Releases &gt;</li>
		</ol>
		</nav>
	</header> 
	<div class="clearer" />
	<t:if t:test="viewingRevision">
		<p class="info">You are currently viewing the revision "${revision}" of this chapter.</p>
	</t:if>
	<t:if t:test="availableContent">
		<t:outputRaw value="content" />
		<p:else>
		This chapter is still empty. Please come back later to see if the author have written something.
		</p:else>
	</t:if>
</div>
</html>
